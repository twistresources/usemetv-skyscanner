<!-- Start Price List -->
<section id="price">
    <div class="container">
        <div class="col-lg-12 section-title-price wow flipInX content-block">
            [[#DESIGNER_MODE]]
	            <div class="ttDesigner-region-label">PRICE CONTENT</div>
            [[/DESIGNER_MODE]]
            [[&CHILD-FLOW-PRICE]]
        </div>
        <div class="price-container">
            <div class="row">
                <div class="col-md-4 col-sm-4 wow flipInY">
                    <div class="price-box text-center gradient">
                        <div class="price-box-info">
                            [[#DESIGNER_MODE]]
                            	<div class="ttDesigner-region-label">PRICE BOX CONTENT</div>
                            [[/DESIGNER_MODE]]
                            [[&CHILD-FLOW-PRICE-BOX-1]]
                        </div>
                        <div class="price-box-offer">
                            [[#DESIGNER_MODE]]
                            	<div class="ttDesigner-region-label">PRICE BOX OFFER CONTENT</div>
                            [[/DESIGNER_MODE]]
                            [[&CHILD-FLOW-PRICE-BOX-OFFER-1]]
                        </div>
                    </div>
                </div>
                <div class="col-md-4 col-sm-4 wow flipInX">
                    <div class="price-box-big text-center gradient">
                        <div class="price-box-info">
                            [[#DESIGNER_MODE]]
                            	<div class="ttDesigner-region-label">PRICE BOX CONTENT</div>
                            [[/DESIGNER_MODE]]
                            [[&CHILD-FLOW-PRICE-BOX-2]]
                        </div>
                        <div class="price-box-offer">
                            [[#DESIGNER_MODE]]
                            	<div class="ttDesigner-region-label">PRICE BOX OFFER CONTENT</div>
                            [[/DESIGNER_MODE]]
                            [[&CHILD-FLOW-PRICE-BOX-OFFER-2]]
                        </div>
                    </div>
                </div>
                <div class="col-md-4 col-sm-4 wow flipInY">
                    <div class="price-box text-center gradient">
                        <div class="price-box-info">
                            [[#DESIGNER_MODE]]
                            	<div class="ttDesigner-region-label">PRICE BOX CONTENT</div>
                            [[/DESIGNER_MODE]]
                            [[&CHILD-FLOW-PRICE-BOX-3]]
                        </div>
                        <div class="price-box-offer">
                            [[#DESIGNER_MODE]]
                            	<div class="ttDesigner-region-label">PRICE BOX OFFER CONTENT</div>
                            [[/DESIGNER_MODE]]
                            [[&CHILD-FLOW-PRICE-BOX-OFFER-3]]
                        </div>
                    </div>
                </div>
            </div>
            <div class="footnote text-center">
                [[#DESIGNER_MODE]]
	                <div class="ttDesigner-region-label">FOOT NOTE CONTENT</div>
                [[/DESIGNER_MODE]]
                [[&CHILD-FLOW-FOOT-NOTE]]
            </div>
        </div>
    </div>
</section>
<!-- End Price List -->
