#map-image {
    height: 480px;
    position: relative;
    background: url('/ttsvr/cropImage/techelite.images.te_img_map.png') no-repeat scroll center center;
    background-size: cover;
}
#map-image a {
    position: absolute;
    width: 100%;
    height: 100%;
}
.navbar-brand, .navbar-brand:hover {
    background: url('/ttsvr/cropImage/techelite.images.te_logo_tech_elite.png') no-repeat scroll center left;
    background-size: 45px;
    
    padding: 25px 0 25px 52px;
    font-size: 16px;
    text-transform: uppercase;
    font-weight: bold;
}
.navbar-brand .span-1 {
    color: #0E64A7;
}
.navbar-brand .span-2 {
    color: #172859;
}
.price-box-info {
    border-bottom: 0;
}
.footnote {
    padding: 10px;
}
.footnote span * {
    display: inline;
}
.cta-btn-big, .cta-btn-medium {
    border-radius: 3px;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    background: #3FA43E;
    color: white;
    display: inline-block;
    margin: 20px 0 10px;
}
.cta-btn-big {
    padding: 16px 30px;
}
.cta-btn-medium {
    padding: 14px 30px;
}
.cta-btn-big a, .cta-btn-medium a {
    text-decoration: none;
    color: white;
    text-transform: uppercase;
}
.cta-btn-big a {
    font-size: 24px;
}
.cta-btn-medium a {
    font-size: 22px;
}
a:active, a:hover, a:focus {
    color: #0076B3;
}
#contact-details {
    background-repeat: repeat;
}
#about {
    padding-bottom: 0;
}
#team .team-box img {
    margin: 0 auto;
}
#contact-details {
    background: none;
}
#contact-details .details {
    margin-top: 0;
}
.details, #contact-details {
    color: inherit;
}
.video-border {
    border: 5px solid #000;
    background: #000;
}
[style="color: #00ccff;"], .highlight,  {
    color: #428bca !important;
}
#contact input, #contact select, #contact textarea {
    border: 1px solid #999;
}
#contact input:focus, #contact select:focus, #contact textarea:focus {
    border-color: #2C3E50;
}</content>
